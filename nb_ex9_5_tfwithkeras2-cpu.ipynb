    "cost_fn = keras.losses.SparseCategoricalCrossentropy(from_logits=True)\n",
    "opt = keras.optimizers.Adam()\n",
    "tr_loss = keras.metrics.Mean(name='train_loss')\n",
    "te_loss = keras.metrics.Mean(name='test_loss')\n",
